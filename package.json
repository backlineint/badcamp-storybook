{
  "name": "@gdwc/components",
  "version": "1.0.0",
  "description": "A library of generic web components that are accessible, framework agnostic, possible to style, and easy to use with data provided by Drupal",
  "files": [
    "dist"
  ],
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "exports": {
    ".": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.umd.js"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git@git.drupal.org:project/gdwc.git"
  },
  "keywords": [
    "drupal"
  ],
  "licenses": [
    {
      "type": "GPL-2.0-or-later",
      "url": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.html"
    }
  ],
  "dependencies": {
    "linkset-menu": "0.0.2",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "vite": "^2.1.0"
  }
}
