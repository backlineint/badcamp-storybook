{
  "name": "@gdwc/components",
  "version": "1.0.1",
  "description": "A library of generic web components that are accessible, framework agnostic, possible to style, and easy to use with data provided by Drupal",
  "files": [
    "dist",
    "index.js",
    "menu.js",
    "gdwc-menu"
  ],
  "main": "./dist/components.umd.js",
  "module": "./dist/components.es.js",
  "exports": {
    ".": {
      "import": "./dist/components.es.js",
      "require": "./dist/components.umd.js"
    },
    "./menu": {
      "import": "./menu.js",
      "require": "./menu.js"
    }
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://git.drupalcode.org/project/gdwc.git"
  },
  "homepage": "https://www.drupal.org/project/gdwc",
  "bugs": {
    "url": "https://www.drupal.org/project/issues/gdwc"
  },
  "keywords": [
    "drupal",
    "web components"
  ],
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "linkset-menu": "0.0.2",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/web-components": "^6.1.21",
    "babel-loader": "^8.2.2",
    "lit-html": "^1.3.0",
    "vite": "^2.1.0"
  }
}
